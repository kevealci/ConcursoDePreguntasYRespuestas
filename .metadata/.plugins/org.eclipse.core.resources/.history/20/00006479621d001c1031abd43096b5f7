package entidades;

public class Opcion {
	
	private boolean valida;
	private String opcion;
	
	/**
	 * @param valida
	 * @param opcion
	 */
	public Opcion(boolean valida, String opcion) {
		this.valida = valida;
		this.opcion = opcion;
	}

	/**
	 * @return the valida
	 */
	public boolean isValida() {
		return valida;
	}

	/**
	 * @param valida the valida to set
	 */
	public void setValida(boolean valida) {
		this.valida = valida;
	}

	/**
	 * @return the opcion
	 */
	public String getOpcion() {
		return opcion;
	}

	/**
	 * @param opcion the opcion to set
	 */
	public void setOpcion(String opcion) {
		this.opcion = opcion;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "Opcion [valida=" + valida + ", opcion=" + opcion + "]";
	}
	
	
	
	
}
